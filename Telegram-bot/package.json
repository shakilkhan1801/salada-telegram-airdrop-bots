{
  "name": "telegram-bot12",
  "version": "1.0.0",
  "description": "Telegram Bot Project",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "start:win": "npm run clean:ports && npm run build && node dist/index.js",
    "clean:ports": "node -e \"const { exec } = require('child_process'); [3001, 3002].forEach(port => exec(`netstat -ano | findstr :${port}`, (err, out) => { if (out) { const pid = out.trim().split(/\\s+/).pop(); if (pid && pid !== '0') exec(`taskkill /F /PID ${pid}`); } }));\"",
    "prebuild": "node src/miniapp-captcha/obfuscate-build.js",
    "build": "tsc && npm run copy-assets",
    "dev": "ts-node src/index.ts",
    "test": "jest --runInBand",
    "lint": "eslint \"**/*.ts\"",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "ci:validate": "npm run typecheck && npm run lint",
    "copy-assets": "node src/scripts/copy-assets.cjs",
    "build:watch": "tsc -w",
    "init-admin": "ts-node src/scripts/init-admin.ts",
    "admin:password": "ts-node src/scripts/change-admin-password.ts"
  },
  "dependencies": {
    "@peculiar/webcrypto": "^1.5.0",
    "@types/bcryptjs": "^2.4.6",
    "@walletconnect/sign-client": "^2.21.9",
    "@walletconnect/types": "^2.21.9",
    "@walletconnect/utils": "^2.21.9",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "canvas": "^3.2.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "dotenv-safe": "^9.1.0",
    "ethers": "^5.8.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "fs-extra": "^11.3.2",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "javascript-obfuscator": "^4.0.2",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.2.1",
    "mongodb": "^6.0.0",
    "nanoid": "^5.1.5",
    "node-cron": "^4.2.1",
    "node-telegram-bot-api": "^0.66.0",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "telegraf": "^4.12.2",
    "uuid": "^13.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.1.2",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.4",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^9.36.0",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "telegram",
    "bot",
    "nodejs"
  ],
  "author": "",
  "license": "ISC",
  "optionalDependencies": {
    "@mongodb-js/zstd": "^2.0.1"
  }
}
